; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="RedBallModarchy"
config/version="1.0000"
run/main_scene="res://scenes/menu/menu.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
run/max_fps=31
boot_splash/bg_color=Color(0.12549, 0, 0.12549, 1)
boot_splash/image="res://images/logo.png"
config/icon="res://images/logo.png"

[autoload]

ModAPI="*res://scripts/globals/mod_api.gd"
InputHelper="*res://scripts/globals/input_helper.gd"
AudioHelper="*res://scripts/globals/audio_helper.gd"
DataHelper="*res://scripts/globals/data_helper.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=1024
window/size/window_height_override=576
window/subwindows/embed_subwindows=false
window/stretch/mode="viewport"

[file_customization]

folder_colors={
"res://_mods/": "purple",
"res://addons/": "red",
"res://audio/": "orange",
"res://images/": "yellow",
"res://scenes/": "green",
"res://scripts/": "blue"
}

[physics]

common/physics_ticks_per_second=31
2d/physics_engine="Rapier2D"
2d/default_gravity=300.0
2d/default_linear_damp=0.0
2d/default_angular_damp=0.0

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
viewport/hdr_2d=true
